      "!src/constants.js",
      "!src/scenes/credentialing/utils/mockData/*.js",
      "!src/utils/fakeState/*.js",
      "!src/utils/testHelpers.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
