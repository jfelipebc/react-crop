  "private": true,
  "devDependencies": {
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.5.0"
  },
  "dependencies": {
    "chai": "4.1.2",
    "chai-enzyme": "1.0.0-beta.1",
    "emerald-ui": "^0.3.8",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.1",
    "react-image-crop": "^6.0.4",
    "react-scripts": "2.1.1",
    "sinon": "6.1.4",
